{
    "name": "rlacerda83/recommendation-system-service",
    "description": "Microservice for products recommendation",
    "keywords": ["recommendation", "microservice", "lumen", "products", "gremlin", "ecommerce"],
    "homepage": "http://github.com/rlacerda83/recommendation-system-service",
    "type": "project",
    "license": "BSD",
    "authors": [
        {
            "name": "Rodrigo Lacerda",
            "email": "r.lacerda83@gmail.com"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~1.0",
        "jeremeamia/superclosure": "~2.0",
        "dingo/api": "2.0.0-alpha1",
        "brightzone/gremlin-php": "*",
        "predis/predis": "^1.1",
        "illuminate/redis": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "mockery/mockery": "dev-master@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
